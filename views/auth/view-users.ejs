<!-- views/auth/view-users.ejs -->

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Log book users</title>
    <link rel="stylesheet" href="/stylesheets/style.css">
</head>

<body>
    <%- include('../partials/_navbar.ejs') %>
    <h1>Log Book Users</h1>
    <a href="/auth/new-user">Create new user account</a>
    <br>
    <ul>
        <% users.forEach((user) => { %>
            <a href="/auth/<%= user._id %>/edit-user">
                <li><%= user.username %></li>
            </a>
        <% }) %>
    </ul>

</body>

</html>